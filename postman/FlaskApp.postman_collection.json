						{
							"listen": "prerequest",
							"script": {
								"id": "6e41811d-e493-49c0-ac21-daddc6febf3e",
								"exec": [
									"var chars = 'abcdefghijklmnopqrstuvwxyz';",
									"var nums = '0123456789'",
									"function random(length, list) {",
									"   var result  = '';",
									"   for ( var i = 0; i < length; i++ ) {",
									"      result += list.charAt(Math.floor(Math.random() * list.length));",
									"   }",
									"   return result;",
									"}",
									"",
									"var random_str = random(7,chars)",
									"var random_phone = '09' + random(9,nums)",
